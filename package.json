{
  "name": "body-measurements",
  "version": "1.0.3",
  "description": "Part of a Laboration in my course at Linnaeus University, software quality and Clean Code",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "run-example": "node examples/exampleWithImperialAndMetric.js",
    "dev": "tsc && node dist/app.js",
    "test": "jest --config ./Jest.config.mjs",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "lint:check": "eslint 'src/**/*.ts' && prettier --check 'src/**/*.ts'",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "precommit": "npm run lint && npm run format:check"
  },
  "keywords": [
    "health",
    "fitness",
    "BMI",
    "BMR",
    "TDEE",
    "body-fat",
    "waist-to-hip-ratio",
    "waist-to-height-ratio",
    "calorie-calculator",
    "weight-goal",
    "health-calculator",
    "nutrition",
    "body-measurements",
    "weight-loss",
    "weight-gain",
    "lean-body-mass",
    "basal-metabolic-rate",
    "total-daily-energy-expenditure",
    "metric",
    "imperial"
  ],
  "author": "Angelica Marker",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.1.6"
  }
}
