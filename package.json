{
  "name": "body-measurements",
  "version": "1.0.0",
  "description": "Part of a Laboration in my course at Linnaeus University, software quality and Clean Code",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "tsc && node dist/app.js",
    "test": "jest --config ./Jest.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "lint:check": "eslint 'src/**/*.ts' && prettier --check 'src/**/*.ts'",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "precommit": "npm run lint && npm run format:check"
  },
  "keywords": [],
  "author": "Angelica Marker",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.1.6"
  }
}
